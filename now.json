{
  "name": "zukrec",
  "alias": "rec.zukdoor.cloud",
  "version": 2,
  "builds": [
    { "src": "./doc/index.html", "use": "@now/static" },
    { "src": "./doc/javascripts/*.min.js", "use": "@now/static" },
    { "src": "./doc/stylesheets/*.min.css", "use": "@now/static" }
  ],
  "routes": [
    {
      "src": "/api/(?<req>.*)",
      "dest": "http://3.80.61.56:8080/zukvnc/api/$req"
    },
    { 
      "src": "/(.*)",
      "dest": "/doc/$1"
    }
  ]
}