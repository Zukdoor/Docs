{
  "name": "zukrec",
  "alias": "rec.zukdoor.cloud",
  "version": 2,
  "builds": [
    { "src": "./doc/index.html", "use": "@now/static" },
    { "src": "./CHANGELOG.md", "use": "@now/md"},
    { "src": "./CHANGELOG-CN.md", "use": "@now/md"}
  ],
  "routes": [
    {
      "src": "/changelog",
      "dest": "/CHANGELOG.html"
    },
    {
      "src": "/changelog/cn",
      "dest": "/CHANGELOG-CN.html"
    },
    {
      "src": "/CHANGELOG.md",
      "dest": "/CHANGELOG.html"
    },
    {
      "src": "/CHANGELOG-CN.md",
      "dest": "/CHANGELOG-CN.html"
    },
    { 
      "src": "/(.*)",
      "dest": "/doc/$1"
    }
  ]
}