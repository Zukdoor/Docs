{
  "name": "zukrec",
  "alias": "rec.zukdoor.cloud",
  "version": 2,
  "builds": [
    { "src": "./doc/index.html", "use": "@now/static" },
    { "src": "./style.css", "use": "@now/static" },
    { "src": "./CHANGELOG.md", "use": "@now/md" },
    { "src": "./CHANGELOG-CN.md", "use": "@now/md" },
    { "src": "./callback.md", "use": "@now/md" }
  ],
  "routes": [
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/callback",
      "dest": "/callback.html",
      "css": "/style.css"
    },
    {
      "src": "/changelog",
      "dest": "/CHANGELOG.html",
      "css": "/style.css"
    },
    {
      "src": "/changelog/cn",
      "dest": "/CHANGELOG-CN.html",
      "css": "/style.css"
    },
    {
      "src": "/CHANGELOG.md",
      "dest": "/CHANGELOG.html",
      "css": "/style.css"
    },
    {
      "src": "/CHANGELOG-CN.md",
      "dest": "/CHANGELOG-CN.html",
      "css": "/style.css"
    },
    {
      "src": "/",
      "dest": "/doc/index.html"
    },
    {
      "src": "/api/(?<req>.*)",
      "dest": "http://39.106.186.45:8711/zukvnc-test/api/$req"
    }
  ]
}
